{
	"name": "FB Data Scraper",
	"description": "This tool extract data from Facebook search people.",
	"version": "1.1",
	"manifest_version": 2,
	"permissions": ["<all_urls>","tabs"],
	"icons": {
		"48": "icons/icon48.png"
	},
  "browser_action": {
  	"default_icon":  "icons/icon20.png",
